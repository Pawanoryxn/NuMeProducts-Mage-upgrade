<catalog_product_view>
   <reference name="head">
      <action method="addItem">
         <type>skin_css</type>
         <name>js/amasty/amconf/css/amconf.css</name>
         <params/>
      </action>
      <action method="addItem">
         <type>skin_js</type>
         <name>js/amasty/amconf/zoomer.js</name>
         <params/>
      </action>
      <action method="addItem" ifconfig="amconf/general/show_title_view">
         <type>skin_js</type>
         <name>js/amasty/amconf/bubleTooltip.js</name>
         <params/>
      </action>
      <action method="addItem" ifconfig="amconf/general/show_title_view">
         <type>skin_css</type>
         <name>js/amasty/amconf/css/bubleTooltip.css</name>
         <params/>
      </action>
      <action method="addItem" ifconfig="amconf/carousel/swipe">
         <type>skin_js</type>
         <name>js/z_amasty/plugins/jquery.touchSwipe.min.js</name>
         <params/>
      </action>
      <action method="addItem" ifconfig="amconf/lightbox/enable">
         <type>skin_css</type>
         <name>js/z_amasty/plugins/fancybox/jquery.fancybox.css</name>
         <params/>
      </action>
      <action method="addItem" ifconfig="amconf/lightbox/enable">
         <type>skin_js</type>
         <name>js/z_amasty/plugins/fancybox/jquery.fancybox.js</name>
         <params/>
      </action>
